  "vim.useSystemClipboard": true,
  "vim.insertModeKeyBindings": [
    {
      //exit insert mode
      "before": ["j", "k"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["é"],
      "after": ["1", "0", "j"]
    },
    {
      "before": ["à"],
      "after": ["1", "0", "k"]
    },
    {
      "before": ["d", "é"],
      "after": ["d", "1", "0", "j"]
    },
    {
      "before": ["d", "à"],
      "after": ["d", "1", "0", "k"]
    },
    {
      "before": ["?"],
      "commands": ["actions.find"]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["é"],
      "after": ["1", "0", "j"]
    },
    {
      "before": ["à"],
      "after": ["1", "0", "k"]
    }
  ],
