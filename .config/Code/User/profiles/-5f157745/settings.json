  "highlight.regexes": {
    "(Todo)": {
      "decorations": [
        {
          "overviewRulerColor": "#a78bfa",
          "backgroundColor": "#a78bfa",
          "color": "#1e1b4b",
          "fontWeight": "bold",
          "borderRadius": "4px"
        }
      ]
    },
    "(Not started)": {
      "decorations": [
        {
          "overviewRulerColor": "#9ca3af",
          "backgroundColor": "#9ca3af",
          "color": "#030712",
          "fontWeight": "bold",
          "borderRadius": "4px"
        }
      ]
    },
    "(In Progress)": {
